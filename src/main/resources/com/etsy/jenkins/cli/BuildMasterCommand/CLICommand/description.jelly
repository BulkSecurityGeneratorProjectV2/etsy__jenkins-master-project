<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" 
         xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

  <span>
   ${%started_by_command(it.getAuthUserName(), it.getAuthUserUrl(), it.getProxyUserName(), it.getProxyUserUrl())}
   <j:if test="${!(it.getDescription() == null || it.getDescription().isEmpty())}">
      <br/>
      ${%description(it.getDescription())}
    </j:if>
  </span>

</j:jelly>

